       right -= 1\n            continue\n\n        # core swap\n        buf[left], buf[right] = buf[right], buf[left]\n\n        left += 1\n        right -= 1\n\n    return buf\n", "fontSize": 16, "fontFamily": 8, "textAlign": "left", "verticalAlign": "middle", "containerId": "r5171XkfxyFbN0fHEZrV9", "originalText": "def symmetric_swap(buf):\n    left, right = 0, len(buf) - 1\n\n    while left < right:\n\n        # advance left until it’s allowed to participate\n        if not valid_left(buf[left]):\n            left += 1\n            continue\n\n        # advance right until it’s allowed to participate\n        if not valid_right(buf[right]):\n            right -= 1\n            continue\n\n        # core swap\n        buf[left], buf[right] = buf[right], buf[left]\n\n        left += 1\n        right -= 1\n\n    return buf\n", "autoResize": true, "lineHeight": 1.25}, {"id": "EhMEnSSwUTp3K0w8WYUxW", "type": "rectangle", "x": -2070.6656051340115, "y": -9174.36297656019, "width": 548.4317841317802, "height": 470, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffffff", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b15", "roundness": {"type": 3}, "seed": 239841157, "version": 8136, "versionNonce": 107607451, "isDeleted": false, "boundElements": [{"type": "text", "id": "P7fJOe3Y"}, {"id": "PaWgKc7KoRlp8l6g4n7ON", "type": "arrow"}], "updated": 1769804826374, "link": null, "locked": false}, {"id": "P7fJOe3Y", "type": "text", "x": -2065.6656051340115, "y": -9169.36297656019, "width": 519.1992797851562, "height": 460, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "transparent", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b16", "roundness": null, "seed": 1855289061, "version": 7234, "versionNonce": 107907643, "isDeleted": false, "boundElements": [], "updated": 1769804826374, "link": null, "locked": false, "text": " symmetric_compare(s: str) -> bool:\nleft, right = 0, len(s) - 1\n\nwhile left < right:\n\n    # --- NORMALIZATION STEP ---\n    # Skip characters left pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[left]):\n        left += 1\n\n    # Skip characters right pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[right]):\n        right -= 1\n\n    # Normalize both sides (e.g., lowercase, strip accents)\n    if normalize(s[left]) != normalize(s[right]):\n        return False\n\n    # --- ADVANCEMENT STEP (SYMMETRIC) ---\n    left += 1\n    right -= 1\n\nreturn True", "rawText": " symmetric_compare(s: str) -> bool:\nleft, right = 0, len(s) - 1\n\nwhile left < right:\n\n    # --- NORMALIZATION STEP ---\n    # Skip characters left pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[left]):\n        left += 1\n\n    # Skip characters right pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[right]):\n        right -= 1\n\n    # Normalize both sides (e.g., lowercase, strip accents)\n    if normalize(s[left]) != normalize(s[right]):\n        return False\n\n    # --- ADVANCEMENT STEP (SYMMETRIC) ---\n    left += 1\n    right -= 1\n\nreturn True", "fontSize": 16, "fontFamily": 8, "textAlign": "left", "verticalAlign": "middle", "containerId": "EhMEnSSwUTp3K0w8WYUxW", "originalText": " symmetric_compare(s: str) -> bool:\nleft, right = 0, len(s) - 1\n\nwhile left < right:\n\n    # --- NORMALIZATION STEP ---\n    # Skip characters left pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[left]):\n        left += 1\n\n    # Skip characters right pointer shouldn't compare.\n    while left < right and not valid_for_compare(s[right]):\n        right -= 1\n\n    # Normalize both sides (e.g., lowercase, strip accents)\n    if normalize(s[left]) != normalize(s[right]):\n        return False\n\n    # --- ADVANCEMENT STEP (SYMMETRIC) ---\n    left += 1\n    right -= 1\n\nreturn True", "autoResize": true, "lineHeight": 1.25}, {"id": "-5sYPRN8fJoaf4k7NiJRc", "type": "rectangle", "x": -1267.1423982112683, "y": -9156.296000648732, "width": 548.4317841317802, "height": 770, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffffff", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b17", "roundness": {"type": 3}, "seed": 1693405765, "version": 8438, "versionNonce": 39286651, "isDeleted": false, "boundElements": [{"type": "text", "id": "FaE90zm0"}, {"id": "Sne5XUDf0cej7KeEGSnGM", "type": "arrow"}], "updated": 1769804826374, "link": null, "locked": false}, {"id": "FaE90zm0", "type": "text", "x": -1262.1423982112683, "y": -9151.296000648732, "width": 501.59930419921875, "height": 760, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "transparent", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b18", "roundness": null, "seed": 1580646821, "version": 7540, "versionNonce": 286314523, "isDeleted": false, "boundElements": [], "updated": 1769804826374, "link": null, "locked": false, "text": "def forward_alignment_compare(A: str, B: str) -> bool:\ni, j = init_positions(A, B)\n# Examples:\n#   for subsequence: i = 0, j = 0\n#   for backspace compare: i = len(A)-1, j = len(B)-1\n#   for version compare: i = 0, j = 0\n\nwhile pointers_have_work(i, j, A, B):\n\n    # --- NORMALIZE POINTER i ---\n    # Move i until it sits on a \"meaningful\" token:\n    #   - skip deleted chars (backspaces)\n    #   - parse a digit-segment (version)\n    #   - skip punctuation\n    i, tokenA = normalize(A, i)\n\n    # --- NORMALIZE POINTER j ---\n    j, tokenB = normalize(B, j)\n\n    # If one side produced a token but the other didn't →\nmismatch.\n    if (tokenA is None) != (tokenB is None):\n        return False\n\n    # If both exist, compare them.\n    if tokenA is not None:   # implies tokenB is not None\n        if tokenA != tokenB:\n            return False\n\n    # --- ADVANCE POINTERS ---\n    # Movement depends on the task:\n    #   backspace compare: i--, j--\n    #   subsequence: if match -> i++, j++; else j++\n    #   version compare: skip dots, move to next segment\n    i, j = advance(i, j)\n\nreturn True\n", "rawText": "def forward_alignment_compare(A: str, B: str) -> bool:\ni, j = init_positions(A, B)\n# Examples:\n#   for subsequence: i = 0, j = 0\n#   for backspace compare: i = len(A)-1, j = len(B)-1\n#   for version compare: i = 0, j = 0\n\nwhile pointers_have_work(i, j, A, B):\n\n    # --- NORMALIZE POINTER i ---\n    # Move i until it sits on a \"meaningful\" token:\n    #   - skip deleted chars (backspaces)\n    #   - parse a digit-segment (version)\n    #   - skip punctuation\n    i, tokenA = normalize(A, i)\n\n    # --- NORMALIZE POINTER j ---\n    j, tokenB = normalize(B, j)\n\n    # If one side produced a token but the other didn't → mismatch.\n    if (tokenA is None) != (tokenB is None):\n        return False\n\n    # If both exist, compare them.\n    if tokenA is not None:   # implies tokenB is not None\n        if tokenA != tokenB:\n            return False\n\n    # --- ADVANCE POINTERS ---\n    # Movement depends on the task:\n    #   backspace compare: i--, j--\n    #   subsequence: if match -> i++, j++; else j++\n    #   version compare: skip dots, move to next segment\n    i, j = advance(i, j)\n\nreturn True\n", "fontSize": 16, "fontFamily": 8, "textAlign": "left", "verticalAlign": "middle", "containerId": "-5sYPRN8fJoaf4k7NiJRc", "originalText": "def forward_alignment_compare(A: str, B: str) -> bool:\ni, j = init_positions(A, B)\n# Examples:\n#   for subsequence: i = 0, j = 0\n#   for backspace compare: i = len(A)-1, j = len(B)-1\n#   for version compare: i = 0, j = 0\n\nwhile pointers_have_work(i, j, A, B):\n\n    # --- NORMALIZE POINTER i ---\n    # Move i until it sits on a \"meaningful\" token:\n    #   - skip deleted chars (backspaces)\n    #   - parse a digit-segment (version)\n    #   - skip punctuation\n    i, tokenA = normalize(A, i)\n\n    # --- NORMALIZE POINTER j ---\n    j, tokenB = normalize(B, j)\n\n    # If one side produced a token but the other didn't → mismatch.\n    if (tokenA is None) != (tokenB is None):\n        return False\n\n    # If both exist, compare them.\n    if tokenA is not None:   # implies tokenB is not None\n        if tokenA != tokenB:\n            return False\n\n    # --- ADVANCE POINTERS ---\n    # Movement depends on the task:\n    #   backspace compare: i--, j--\n    #   subsequence: if match -> i++, j++; else j++\n    #   version compare: skip dots, move to next segment\n    i, j = advance(i, j)\n\nreturn True\n", "autoResize": true, "lineHeight": 1.25}, {"id": "PaWgKc7KoRlp8l6g4n7ON", "type": "arrow", "x": -2353.1529575495015, "y": -8172.195560703039, "width": 297.32442920439144, "height": 522.3499336002669, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffc9c9", "fillStyle": "solid", "strokeWidth": 2, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b19", "roundness": {"type": 2}, "seed": 1335444741, "version": 11140, "versionNonce": 788132443, "isDeleted": false, "boundElements": [], "updated": 1769920370137, "link": null, "locked": false, "points": [[0, 0], [297.32442920439144, -522.3499336002669]], "lastCommittedPoint": null, "startBinding": {"elementId": "knYXUKs1Tv0ZvkTzaGwbR", "focus": 0.02477957864559154, "gap": 1}, "endBinding": {"elementId": "EhMEnSSwUTp3K0w8WYUxW", "focus": 0.29419706181697725, "gap": 12.217395366135257}, "startArrowhead": null, "endArrowhead": "arrow", "elbowed": false}, {"id": "Sne5XUDf0cej7KeEGSnGM", "type": "arrow", "x": -2137.614716063392, "y": -8110.931982199258, "width": 838.8544952211205, "height": 299.04860560565794, "angle": 0, "strokeColor": "#1e1e1e