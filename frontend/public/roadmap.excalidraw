atures(self, temperatures: List[int]) ->\n    stack = []\n    answer = [0] * len(temperatures)\n    \n    for i in range(len(temperatures)):\n        while stack and temperatures[stack[-1]] < temperatures[i]:\n            j = stack.pop()\n            answer[j] = i - j\n        stack.append(i)\n    \n    return answer", "autoResize": true, "lineHeight": 1.25}, {"id": "ewkSuJ0oqL5d1wwucEzOm", "type": "arrow", "x": -7187.563726885377, "y": -2786.5020727064157, "width": 558.8576567552236, "height": 432.14370247108855, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffc9c9", "fillStyle": "solid", "strokeWidth": 2, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b64", "roundness": {"type": 2}, "seed": 762543914, "version": 11900, "versionNonce": 2097817723, "isDeleted": false, "boundElements": [], "updated": 1769920370215, "link": null, "locked": false, "points": [[0, 0], [-558.8576567552236, 432.14370247108855]], "lastCommittedPoint": null, "startBinding": {"elementId": "tGUmYf5Zu19LkbCUFTqwc", "focus": 0.05376017404251006, "gap": 21.325971347634898}, "endBinding": {"elementId": "R_UvTO0Tn4GhvZxuzhnMz", "focus": 0.4702699001856592, "gap": 1}, "startArrowhead": null, "endArrowhead": "arrow", "elbowed": false}, {"id": "yogazSWGpG3DcmOKCab8C", "type": "rectangle", "x": -9608.3338867492, "y": -3433.7339983999245, "width": 518.1733986417235, "height": 335, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffffff", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "b66", "roundness": {"type": 3}, "seed": 1812161206, "version": 7788, "versionNonce": 1550259067, "isDeleted": false, "boundElements": [{"type": "text", "id": "EnQzHkLe"}, {"id": "uUPkyirEBmcY-05zin0zR", "type": "arrow"}], "updated": 1769804826375, "link": null, "locked": false}, {"id": "EnQzHkLe", "type": "text", "x": -9603.3338867492, "y": -3428.73399839992