t contains at most k distinct characters.\n\ndef find_longest_substring(s, k):\n    counts = defaultdict(int)\n    left = ans = 0\n    for right in range(len(s)):\n        counts[s[right]] += 1\n        while len(counts) > k:\n            counts[s[left]] -= 1\n            if counts[s[left]] == 0:\n                del counts[s[left]]\n            left += 1\n        \n        ans = max(ans, right - left + 1)\n    \n    return ans", "autoResize": true, "lineHeight": 1.25}, {"id": "8fPDpfBXNA6tUcomg2JEZ", "type": "arrow", "x": -294.456304932169, "y": -5560.430569438742, "width": 919.4388703882217, "height": 310.3682509528435, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#f8f1ee", "fillStyle": "solid", "strokeWidth": 1, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "a7", "roundness": {"type": 2}, "seed": 736679327, "version": 7185, "versionNonce": 1604054907, "isDeleted": false, "boundElements": [], "updated": 1769920370126, "link": null, "locked": false, "points": [[0, 0], [919.4388703882217, 310.3682509528435]], "lastCommittedPoint": null, "startBinding": {"elementId": "A5oH33n95GJe6aNAeDuu_", "focus": 0.18256233979957848, "gap": 1.000347893848867}, "endBinding": {"elementId": "Hhdyc1CwN4H_yj2bfcY2N", "focus": -0.05054522611652708, "gap": 8.555515837663734}, "startArrowhead": null, "endArrowhead": "arrow", "elbowed": false}, {"id": "tyA9RgoXyWuIuMS0_28Wu", "type": "arrow", "x": -1788.5154969071355, "y": -5691.564595331292, "width": 872.5944772333816, "height": 4.118372677260595, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#f8f1ee", "fillStyle": "solid", "strokeWidth": 1, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "a8", "roundness": {"type": 2}, "seed": 77253055, "version": 8217, "versionNonce": 2012283451, "isDeleted": false, "boundElements": [], "updated": 1769920370122, "link": null, "locked": false, "points": [[0, 0], [872.5944772333816, -4.118372677260595]], "lastCommittedPoint": null, "startBinding": {"elementId": "uoyHFXm1xWeyjJFV5337G", "focus": 0.1636566425115229, "gap": 15.691477917062498}, "endBinding": {"elementId": "A5oH33n95GJe6aNAeDuu_", "focus": -0.1115615019901024, "gap": 1}, "startArrowhead": null, "endArrowhead": "arrow", "elbowed": false}, {"id": "1LSvUGUfNvG18IBNcNQSb", "type": "rectangle", "x": 797.9119238380435, "y": -5940.764359190455, "width": 422.8921015920963, "height": 470, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#ffffff", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "aA", "roundness": {"type": 3}, "seed": 1221895679, "version": 6673, "versionNonce": 785257915, "isDeleted": false, "boundElements": [{"type": "text", "id": "DcNAxQ4A"}, {"id": "PHA70XPx6-1shNsVwip5o", "type": "arrow"}], "updated": 1769804826374, "link": null, "locked": false}, {"id": "DcNAxQ4A", "type": "text", "x": 802.9119238380435, "y": -5935.764359190455, "width": 395.99945068359375, "height": 460, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "transparent", "fillStyle": "solid", "strokeWidth": 0.5, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "index": "aB", "roundness": null, "seed": 1215139359, "version": 5774, "versionNonce": 697935451, "isDeleted": false, "boundElements": [], "updated": 1769804826374, "link": null, "locked": false, "text": "def bucketSort(self, s: str) -> str:\n        # Step 1: count frequencies manually\n        freq = {}\n        for ch in s:\n            freq[ch] = freq.get(ch, 0) + 1\n\n        # Step 2: create buckets — index =\nfrequency, value = list of chars\n        buckets = [[] for _ in range(len(s) +\n1)]\n        for ch, count in freq.items():\n            buckets[count].append(ch)\n\n        # Step 3: build result from highest\nfrequency down\n        result = []\n        for i in range(len(buckets) - 1, 0, -\n1):\n            for ch in buckets[i]:\n                result.append(ch * i)  #\nrepeat character i times\n\n        return \"\".join(result)", "rawText": "def bucketSort(self, s: str) -> str:\n        # Step 1: count frequencies manually\n        freq = {}\n        for ch in s:\n            freq[ch] = freq.get(ch, 0) + 1\n\n        # Step 2: create buckets — index = frequency, value = list of chars\n        buckets = [[] for _ in range(len(s) + 1)]\n        for ch, count in freq.items():\n            buckets[count].append(ch)\n\n        # Step 3: build result from highest frequency down\n        result = []\n        for i in range(len(buckets) - 1, 0, -1):\n            for ch in buckets[i]:\n                result.append(ch * i)  # repeat character i times\n\n        return \"\".join(result)", "fontSize": 16, "fontFamily": 8, "textAlign": "left", "verticalAlign": "middle", "containerId": "1LSvUGUfNvG18IBNcNQSb", "originalText": "def bucketSort(self, s: str) -> str:\n        # Step 1: count frequencies manually\n        freq = {}\n        for ch in s:\n            freq[ch] = freq.get(ch, 0) + 1\n\n        # Step 2: create buckets — index = frequency, value = list of chars\n        buckets = [[] for _ in range(len(s) + 1)]\n        for ch, count in freq.items():\n            buckets[count].append(ch)\n\n        # Step 3: build result from highest frequency down\n        result = []\n        for i in range(len(buckets) - 1, 0, -1):\n            for ch in buckets[i]:\n                result.append(ch * i)  # repeat character i times\n\n        return \"\".join(result)", "autoResize": true, "lineHeight": 1.25}, {"id": "PHA70XPx6-1shNsVwip5o", "type": "arrow", "x": -294.06589956396454, "y": -5672.632023904874, "width": 1090.9777669703867, "height": 2.64577717521297, "angle": 0, "strokeColor": "#1e1e1e", "backgroundColor": "#f8f1ee", "fillStyle": "solid", "strokeWidth": 1, "strokeStyle": "solid", "roughness": 1, "opacity": 100, "groupIds": [], "frameId": null, "in